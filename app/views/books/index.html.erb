<h1>Books</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Body</th>
    <th colspan="3"></th>
  </tr>
</table>


<h1>New Books</h1>

<%= form_with model: Book.new do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Book</h4>
  <%= f.text_area :body %>

  <div><%= f.submit 'Create Book' %></div>
<% end %>